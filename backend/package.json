{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for VirtualFeed - AI Generated Video Platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node --prefer-ts-exts src/index.ts",
    "dev:scrape": "ts-node --prefer-ts-exts src/index.ts --enable-scraping",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scrape": "ts-node --prefer-ts-exts src/scripts/scrape.ts",
    "lang:reindex": "ts-node --prefer-ts-exts src/scripts/reindexVideoLanguages.ts",
    "reddit:media:refresh": "ts-node --prefer-ts-exts src/scripts/refreshRedditMediaSources.ts",
    "reddit:thumbnails:cleanup": "ts-node --prefer-ts-exts src/scripts/cleanupRedditThumbnails.ts",
    "reddit:thumbnails:cleanup:dry": "ts-node --prefer-ts-exts src/scripts/cleanupRedditThumbnails.ts --dry-run",
    "scrape:youtube": "ts-node --prefer-ts-exts scripts/scrape-youtube.ts",
    "test:youtube": "ts-node --prefer-ts-exts scripts/test-youtube-scraper.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cheerio": "^1.0.0",
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^22.13.8",
    "axios": "^1.11.0",
    "cheerio": "^1.1.0",
    "cld3-asm": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fluent-ffmpeg": "^2.1.3",
    "mysql2": "^3.12.0",
    "sequelize": "^6.37.5",
    "snoowrap": "^1.23.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "open": "^10.1.0"
  }
}
